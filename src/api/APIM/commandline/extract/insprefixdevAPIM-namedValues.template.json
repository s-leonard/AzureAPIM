{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string"
    },
    "PolicyXMLSasToken": {
      "type": "string"
    },
    "PolicyXMLBaseUrl": {
      "type": "string"
    },
    "NamedValues": {
      "type": "object"
    }
  },
  "resources": [
    {
      "properties": {
        "tags": [],
        "secret": false,
        "displayName": "customersbackend",
        "value": "[parameters('NamedValues').customersbackend]"
      },
      "name": "[concat(parameters('ApimServiceName'), '/customersbackend')]",
      "type": "Microsoft.ApiManagement/service/properties",
      "apiVersion": "2019-01-01"
    }
  ]
}